{
  "manifest_version": 2,
  "name": "Wikia Assistant Extension",
  "description": "This extension makes it easier to use Wikia",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Visit wikia.com"
  },
  "permissions": [
  ],
  "background": {
    "scripts": [
      "lib/main.js"
    ]
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
